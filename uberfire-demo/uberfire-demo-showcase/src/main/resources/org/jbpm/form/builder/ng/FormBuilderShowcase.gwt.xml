  <inherits name="org.jboss.errai.ui.UI" />
  <inherits name="com.google.gwt.xml.XML" />
  <!--inherits name='com.googlecode.gchart.GChart'/-->

  <!-- Uberfire dependencies -->
  <inherits name="org.uberfire.UberfireJS"/>
  <inherits name="org.uberfire.UberfireWorkbench"/>
  <inherits name="org.uberfire.UberfireWidgetsCore"/>
  <inherits name="org.uberfire.UberfireWidgetsCommons"/>
  <inherits name="org.uberfire.UberfireBackend"/>
  <inherits name="org.uberfire.security.UberfireSecurityClient"/>

  <!-- HTML editor dependencies -->
  <inherits name='com.gwtent.GwtEnt' />

  <inherits name='org.jbpm.console.ng.ht.JbpmConsoleNGHumanTasksAPI'/>
  <inherits name='org.jbpm.console.ng.bd.JbpmConsoleNGBusinessDomainAPI'/>
  <inherits name='org.jbpm.console.ng.pr.JbpmConsoleNGProcessRuntimeAPI'/>

