  <inherits name='org.guvnor.common.services.GuvnorServicesAPI'/>
  <inherits name='org.guvnor.common.services.workingset.GuvnorWorkingsetAPI'/>

  <!-- Specify the application specific style sheet. -->
  <stylesheet src='css/uberfire-showcase.css'/>
  <stylesheet src='css/chome-override.css'/>
  <stylesheet src='css/console.css'/>
  <stylesheet src='css/bootstrap.min.css'/>

  <!-- Specify the paths for translatable code -->
  <source path='client'/>
  <source path='shared'/>
  
  
  <inherits name="com.plugtree.training.demo.UberfireDemoClient"/>
  <inherits name="com.plugtree.training.api.UberfireDemoAPI"/> 
  
  
